
<%= form_for @stickey_advertisement, :html => {:class => "form-horizontal", :multipart => true } do |f| %>





            <div class="form-group">
            <label class="col-md-3 control-label"> Image Upload</label>
            <div class="col-md-9">
    <%= f.file_field :avatar1 %>
     <%= image_tag @stickey_advertisement.avatar1.url(:thumb) %>
    </div>
        </div>

<div class="row">
          <div class="col-md-offset-3 col-md-9">
            <%= f.submit :class=>"btn blue" %>
             <a href="/stickey_advertisements" class="btn default" role="button"> Cancel </a>
          </div>
        </div>
<% end %>
